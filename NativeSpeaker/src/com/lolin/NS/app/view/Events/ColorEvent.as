package com.lolin.NS.app.view.Events
{
	import flash.events.Event;
	
	public class ColorEvent extends Event
	{
		static public const COLOREVENT:String = "NSCOLOREVENT";
		public var _color:uint = 0x000000;
		
		public function ColorEvent(color:uint, type:String, bubbles:Boolean=false, cancelable:Boolean=false)
		{
			super(type, bubbles, cancelable);
			this._color = color;
		}
	}
}